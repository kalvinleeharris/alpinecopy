    api.setAlt('#eh img[src*="eh"]', 'Equal Housing Lender');
    api.setAlt('#fdic img[src*="fdic"]', 'Member F D I C');
    api.setAlt('#ncua img[src*="ncua"]', 'NCUA Federal Insurance');
    api.setAlt('#esi img[src*="esi"]', 'ESI ExcessShare');
    //background image
    api.addComplianceIndent('#eh:empty, #eh a:empty[href*="eh"]', 'append', 'span', 'Equal Housing Lender');
    api.addComplianceIndent('#fdic:empty, #fdic a:empty[href*="fdic"]', 'append', 'span', 'Member F D I C');
    api.addComplianceIndent('#ncua:empty, #ncua a:empty[href*="ncua"]', 'append', 'span', 'NCUA Federal Insurance');
    api.addComplianceIndent('#esi:empty, #esi a:empty[href*="excessshare"]', 'append', 'span', 'ESI ExcessShare');
    //Alternate text
    ele.outerFind('img[src$="fileAsset"][alt$=".jpg"], img[src$="fileAsset"][alt$=".png"]').each(function() {
      $ae(this).attr({
        'alt': $ae(this).attr('alt').replace('.jpg', '').replace('.png', '').replace(/-/g, ' ').replace(/_/g, ' ').replace(/\s*\b\w*\d\d\d\d\d\w*\b/g, '')
      });
