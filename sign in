      var txt;
      var parClass;
      var lbl;
      if ($ae(this).is($ae(this).parent().find('input:eq(0)'))) {
        if ($ae(this).parent().hasClass('phone') == true) {
          parClass = 'Phone -';
          lbl = 'Area Code';

        }
        if ($ae(this).parent().hasClass('ssn') == true) {
          parClass = 'SSN -';
          lbl = 'First 3';
        }
        if ($ae(this).parent().hasClass('date') == true) {
          parClass = 'Date -';
          lbl = 'Month';
        }
        txt = parClass +' '+ lbl;
      }
      if ($ae(this).is($ae(this).parent().find('input:eq(1)'))) {
        if ($ae(this).parent().hasClass('phone') == true) {
          parClass = 'Phone -';
          lbl = 'First 3';
        }
        if ($ae(this).parent().hasClass('ssn') == true) {
          parClass = 'SSN -';
          lbl = 'Middle 2';
        }
        if ($ae(this).parent().hasClass('date') == true) {
          parClass = 'Date -';
          lbl = 'Day';
        }
        txt = parClass +' '+ lbl;
      }
      if ($ae(this).is($ae(this).parent().find('input:eq(2)'))) {
        if ($ae(this).parent().hasClass('phone') == true) {
          parClass = 'Phone -';
          lbl = 'Last 4';
        }
        if ($ae(this).parent().hasClass('ssn') == true) {
          parClass = 'SSN -';
          lbl = 'Last 4';
        }
        if ($ae(this).parent().hasClass('date') == true) {
          parClass = 'Date -';
          lbl = 'Year';
        }
        txt = parClass +' '+ lbl;
      }

      api.addComplianceIndent($ae(this), 'before', 'label', txt);
    });

