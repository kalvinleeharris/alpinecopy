    //////////// 

    //decorative headers
    api.setPresentation('h6, h5, #locList .branchInfo :header, form :header, table :header, .comparison-chart :header');
    //hide headers missing text
    ele.outerFind('h1, h2, h3, h4, h5, h6').each(function() {
      if ($ae.trim($ae(this).text()).length === 0) {
        if ($ae(this).children().length === 0) {
          api.hideFromAT($ae(this));
        }
        if ($ae(this).clone().find('b:empty').remove().end().find('strong:empty').remove().end().find('span:empty').remove().end().children().length === 0) {
          api.hideFromAT($ae(this));
        }
      }
    });

    ///////////// 
    ///iFrames//
    /////////// 

    ele.outerFind('iframe').not('[title]').each(function() {
      if (!this.hasAttribute('src')) {
        api.hideFromAT($ae(this));
      } else {
        if ($ae(this).attr('src') == 'about:blank') {
          api.hideFromAT($ae(this));
        }
        if ($ae(this).attr('src').indexOf('insight.adsrvr.org') > -1 || $ae(this).attr('src').indexOf('adservices.brandcdn.com') > -1) {
          api.setTitle($ae(this), ad);
        }
        if ($ae(this).attr('src').indexOf('e.issuu.com') > -1 || $ae(this).attr('src').indexOf('vimeo.com') > -1 || $ae(this).attr('src').indexOf('youtube.com') > -1) {
          api.setTitle($ae(this), video);
        }
        if ($ae(this).attr('src').indexOf('google.com/recaptcha') > -1) {
          api.setTitle($ae(this), recapwidget);
        }
        if ($ae(this).closest('#map, #map-canvas, #mapContainer, #dummy').length > 0) {
          api.setTitle($ae(this), map);
        }
      }
    });

    /////////////
    ///Dialogs//
    ///////////

    // MODALS

    function setupModal(trigger, wrapper, content, close, title, return_target) {
      // ADD TITLE AND ROLE=DIALOG
      api.setTitle(wrapper, title);
      api.setDialog(wrapper);
      // ADD RETURN CODE TO CLOSE BUTTON
      if(ele.outerFind(close).not('button, input') == true){
      	api.convertToBtn(ele.outerFind(close));
      }
      if(ele.outerFind(close).text().toLowerCase().trim() == 'x')
      {
        api.addComplianceIndent(close, 'append', 'span', ' - Close');
      }
      ele.outerFind(close).click(function() {
        AudioEye.focusElement(return_target);
      });

      // SET OUR TABBABLE ELEMENTS
      var tab_container = $ae(wrapper);
      var tab_first;
      var tab_last;

      tab_first = tab_container.find(':tabbable:first');
      tab_last = tab_container.find(':tabbable:last');

      if(content == 'iframe' && tab_container.find('iframe').length > 0){
        if(tab_container.find(close+', iframe').first().is(tab_container.find(close)) == true)
        {
          tab_first = tab_container.find(close);
          tab_last = tab_container.find(content).contents().find(':tabbable').not('#ae-skip-to-anchor').last();
        } else {
          tab_first = tab_container.find(content).contents().find(':tabbable').not('#ae-skip-to-anchor').first();
          tab_last = tab_container.find(close);
        }
      }
      // TRAP TAB IN MODAL

      tab_first.attr('data-ae-ignore-keys', 'tab');
      tab_last.attr('data-ae-ignore-keys', 'tab');

      tab_first.off('keydown').on('keydown', function(e) {
        if (e.keyCode === 9 && e.shiftKey === true) {
          AudioEye.focusElement(tab_last);
          e.preventDefault();
          if ($ae('body').hasClass('ae-player')) {
          	e.stopImmediatePropagation();
          }
        }
      });

      tab_last.off('keydown').on('keydown', function(e) {
        if (e.keyCode === 9 && e.shiftKey === false) {
          AudioEye.focusElement(tab_first);
          e.preventDefault();
          if ($ae('body').hasClass('ae-player')) {
          	e.stopImmediatePropagation();
          }
        }
      });
      if ($ae('body').hasClass('ae-player')) {
        window.setTimeout(function() {
          AudioEye.focusElement(tab_first);
        }, 700);
      } else {
        AudioEye.focusElement(tab_first);
      }
    }

    function aeModal(trigger, wrapper, content, close) {
      // ATTACH MODAL EVENT TO OUR MODAL TRIGGER
      ele.outerFind(trigger).click(function() {
        // SET UP OUR VARIABLES
        var return_target = this;
        var modal_title = $ae(this).safeText();
        // RUN CONTENT CHECK / MANAGEMENT IN AN INTERVAL LOOP
        var modal_interval = window.setInterval(function() {
          // MAKE SURE OUR MODAL EXISTS
          if ($ae(wrapper).find(content).length > 0) {
            window.clearInterval(modal_interval);
            // MANAGE OUR CONTENT BASED ON NEEDS
            switch (content) {
              case 'iframe':
                ele.outerFind(wrapper).find(content).load(function() {
                  setTimeout(function() {
                    setupModal(trigger, wrapper, content, close, modal_title, return_target);
                  }, 300);
                });
                break;
              default:
                setTimeout(function() {
                  setupModal(trigger, wrapper, content, close, modal_title, return_target);
                }, 300);
            }
            if (modal_interval > 10000) {
              window.clearInterval(modal_interval);
            }
          }
        }, 300);
      });
    }
    // -------------- START ---------------------//
    ele.outerFind('a.requestInfo, .questions a').addClass('iframe');
    aeModal('a.calcPopup, a.iframe, a.fancybox', '.mfp-wrap, #fancybox-wrap, .fancybox-wrap', 'iframe', '.mfp-close, .fancybox-close, #fancybox-close');
	aeModal('.warn, .confirm', '#popup_container, .modal', 'div.modal-content', '#popup_container :button, .modal.close');
    aeModal('.searchbox-toggle', '.mfp-wrap', 'div.mfp-content', '.mfp-close');
    
    // TOOL BUG UPDATES
    
    ele.outerFind('.fedCalcDisclaimer').addClass('ae-exclude');

    //////////////
    ///keyboard//
    ////////////

    api.addKeyboardClick(ele.outerFind('[role="button"]').not('a[href], button, input'));

    var end = Date.now();
    console.log('Manual remediation loading time ', end - start);

  });
